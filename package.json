{
  "name": "aws-deploy-assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --config vite.config.analyze.js",
    "build:production": "NODE_ENV=production vite build",
    "build:size-check": "npm run build && node scripts/analyze-bundle.js",
    "analyze-bundle": "node scripts/analyze-bundle.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:production": "vite preview --mode production",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "performance-audit": "npm run build && npx lighthouse http://localhost:4173 --view",
    "deploy-prep": "node scripts/deploy-prep.js"
  },
  "dependencies": {
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.27",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.3.3",
    "terser": "^5.43.1",
    "vite": "^4.4.5",
    "vitest": "^3.2.4"
  }
}
